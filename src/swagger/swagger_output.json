      "post": {
        "tags": ["User"],
        "description": "Rota para cadastro de usuário",
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "schema": {
              "type": "object",
              "properties": {
                "name": {
                  "example": "John Doe"
                },
                "cpf": {
                  "example": "123.456.789-00"
                },
                "birth": {
                  "example": "01/23/1994"
                },
                "email": {
                  "example": "johndoe@example.com"
                },
                "password": {
                  "example": "teste123"
                },
                "cep": {
                  "type": "string",
                  "example": "12345678"
                },
                "qualified": {
                  "example": "sim"
                }
              }
            }
          }
        ],
        "responses": {
          "201": {
            "description": "OK"
          },
          "400": {
            "description": "Bad Request"
          },
          "500": {
            "description": "Internal Server Error"
          }
        }
      },
      "get": {
        "tags": ["User"],
        "description": "Rota para listar todos os usuários",
